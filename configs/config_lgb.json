{
    "data": {
        "data_name": "data.csv", 
        "out_dir": "data", 
        "test_size": 0.15, 
        "target": "Churn", 
        "cat_cols": [
            "gender",
            "Partner",
            "Dependents",
            "PhoneService",
            "MultipleLines",
            "InternetService",
            "OnlineSecurity",
            "OnlineBackup",
            "DeviceProtection",
            "TechSupport",
            "StreamingTV",
            "StreamingMovies",
            "Contract",
            "PaperlessBilling",
            "PaymentMethod"
        ]
    },

    "model": {
        "objective": "binary",
        "boosting_type": "gbdt",
        "eval_metric": "logloss",
        "n_estimators": 1000,
        "verbose": -100,
        "random_state": 17,
    
        "scale_pos_weight": 1.5,
        
        "learning_rate": 0.03,
    
        "num_leaves": 40,
        "values_depth": -1,
    
        "reg_alpha": 20,
        "reg_lambda": 20,
    
        "subsample": 0.5,
        "col_sample_bytree": 0.5
    }
}